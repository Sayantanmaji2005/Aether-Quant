# AetherQuant production environment template
# Copy to your deployment secret store or .env file and replace placeholders.

AETHERQ_LOG_LEVEL=INFO
AETHERQ_DEFAULT_SYMBOL=SPY
AETHERQ_INITIAL_CASH=100000
AETHERQ_COMMISSION_BPS=1.0
AETHERQ_SLIPPAGE_BPS=0.5

# API security
AETHERQ_API_KEY=replace_with_trader_api_key
AETHERQ_ADMIN_API_KEY=replace_with_admin_api_key

# Persistence (PostgreSQL recommended in production)
AETHERQ_DATABASE_URL=postgresql://aetherquant_user:change_me@db-host:5432/aetherquant

# API abuse protection
AETHERQ_RATE_LIMIT_PER_MINUTE=120

# Live broker settings
AETHERQ_LIVE_BROKER_PROVIDER=generic-rest
AETHERQ_LIVE_BROKER_ENDPOINT=https://broker.example
AETHERQ_LIVE_BROKER_KEY_ID=
AETHERQ_LIVE_BROKER_TOKEN=replace_with_broker_token_or_secret
AETHERQ_LIVE_BROKER_DRY_RUN=false
